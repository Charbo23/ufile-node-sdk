{
  "opts": {
    "encoding": "utf8",
    "destination": "docs"
  },
  "source": {
    "include": ["src/index.js"]
},
  "plugins": [
    "node_modules/jsdoc-babel"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}